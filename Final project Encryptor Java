        document.getElementById('warning').src = "http://54.68.137.106/9nr13/Images/warning2.png";
	}

function imageChange4() {
        document.getElementById('contact').src = "http://54.68.137.106/9nr13/Images/contact2.png";
	}

function imageChange5() {
        document.getElementById('encrypt').src = "http://54.68.137.106/9nr13/Images/encrypt2.png";
	}

